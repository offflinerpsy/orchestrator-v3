{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/check-port.js",
    "dev": "cross-env NODE_OPTIONS=\"--trace-uncaught --unhandled-rejections=strict\" next dev",
    "build": "next build",
    "prestart": "node scripts/check-port.js",
    "start": "cross-env NODE_OPTIONS=\"--trace-uncaught --unhandled-rejections=strict\" next start",
    "lint": "next lint",
    "test:smoke": "playwright test",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "revisor:build": "pnpm build",
    "revisor:start": "cross-env NODE_OPTIONS=\"--trace-uncaught\" next start -p 3002",
    "revisor:wait": "wait-on http://127.0.0.1:3002",
    "revisor:test": "playwright test -c playwright.audit.config.ts",
    "revisor:lhci": "lhci autorun --collect.url=http://127.0.0.1:3002 --upload.target=filesystem --upload.outputDir=reports/lhci",
    "revisor:axe": "node tests/a11y/run-axe.mjs",
    "revisor:all": "concurrently -k -s first \"pnpm revisor:start\" \"pnpm revisor:wait && pnpm revisor:test && pnpm revisor:lhci && pnpm revisor:axe\"",
    "revisor:report": "node scripts/audit-summarize.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.20.0",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.546.0",
    "next": "15.0.3",
    "node-html-parser": "^7.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-resizable-panels": "^3.0.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "v0-sdk": "^0.14.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
}
